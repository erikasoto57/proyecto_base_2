<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cetys.angelarambula.android.practica_01_base" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.cetys.angelarambula.android.practica_02_base.view.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.cetys.angelarambula.android.practica_02_base.view.GreetingActivity"
            android:label="@string/title_activity_greeting" />
        <activity
            android:name="com.cetys.angelarambula.android.practica_02_base.view.DataListingActivity"
            android:label="@string/title_activity_data_listing" />
        <activity
            android:name="com.cetys.angelarambula.android.practica_02_base.view.Details"
            android:label="@string/title_activity_details" >
        </activity>
    </application>

</manifest>
